<Application x:Class="snake_wpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:snake_wpf"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#80000000</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackground">#80000000</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#F5F6F7</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#F5F6F7</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#AA000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">\Assets\#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
